newmakefile:
	makefile -m -f $(MAKEFILE)  -s INC $(INC) -s LINC $(LINC)
#bottom#

$(MAINS)     : $(LIB)

install      : $(MAINS) 
	for i in $(MAINS) ; do cpset $$i $(BIN) ; done
	
###########
	
listings     : $(SOURCES)
	pr -h "     stat.d.    " $(SOURCES) | lp

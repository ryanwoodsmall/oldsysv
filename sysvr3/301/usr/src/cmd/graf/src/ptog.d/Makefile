###
### Standard Constants
###

ROOT     =
LROOT    =
BIN      = $(ROOT)/$(LROOT)/usr/bin/graf
LIB      = ../../lib/glib.a

INC	 = $(ROOT)/usr/include
LINC	 = ../../include

CC       = cc
CFLAGS   = -O -I$(INC) -I$(LINC)  $(FFLAG)
LDFLAGS  = $(LIB) -lm -i -s


#top#
# Generated by makefile 1.44    Tue Mar 12 12:50:09 EST 1985

MAKEFILE = Makefile


MAINS = ptog

OBJECTS =  linemod.o ptog.o ptogarcpt.o

SOURCES =  linemod.c ptog.c ptogarcpt.c

ALL:		$(MAINS)

ptog:		ptog.o linemod.o ptogarcpt.o 
	$(CC) $(CFLAGS)  -o ptog  ptog.o linemod.o ptogarcpt.o   $(LDFLAGS)


linemod.o:	 

ptog.o:		 $(INC)/stdio.h $(LINC)/util.h \
		 $(INC)/ctype.h $(LINC)/gsl.h \
		 $(LINC)/gpl.h $(LINC)/debug.h \
		 $(LINC)/errpr.h 

ptogarcpt.o:	 


clean:
	rm -f $(OBJECTS)
	
clobber:	
	rm -f $(OBJECTS) $(MAINS)

newmakefile:
	makefile -m -f $(MAKEFILE)  -s INC $(INC) -s LINC $(LINC)
#bottom#

$(MAINS)     : $(LIB)

install      : $(MAINS) 
	cpset ptog $(BIN)
	
###########
	
listings     : $(SOURCES)
	pr -h "     ptog.d.    " $(SOURCES) | lp

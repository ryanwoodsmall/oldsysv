clean:
	rm -f $(OBJECTS)
	
clobber:	
	rm -f $(OBJECTS) $(LIBRARY)

newmakefile:
	makefile -m -f $(MAKEFILE) -L $(LIBRARY)  -s INC $(INC) -s LINC $(LINC)
#bottom#
install      : $(LIBRARY)
	cpset $(LIBRARY) $(LIB)
	
###########
	
listings     : $(SOURCES)
	pr -h "     glib.d     " $(SOURCES) | lp

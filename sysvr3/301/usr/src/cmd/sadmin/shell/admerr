#ident	"@(#)sadmin:shell/admerr	2.3"
#	Issue a standard error message for an administrative command.

#!	chmod +x ${file}

if [ $# -lt 2 ]
then
	echo >&2 "Usage:  $0 command-name message ..."
	exit 1
fi

case $0 in
*/admerr | admerr )
	type='ADMIN COMMAND'
	;;
* )
	type=SOFTWARE
esac

cmd=$1
shift
echo >&2 "
${type} ERROR:	Call Customer Service.
	${cmd}:  $*\\n"

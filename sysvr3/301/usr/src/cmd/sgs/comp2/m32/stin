					"	pushw	ZDL\n"
					"	pushw	AR\n"
					"	pushw	ZDR\n"
					"	call	&4,_fsubd\n"
					"	movw	A1,AL\n"
					"	movw	ZD1,ZDL\n":60;
-=[d] IAWD[d],DAWD[d]	{$A $> $1 }
					"Y	movw	A(LL),A3\n"

*=[cuc] BDST,BSRC	{$L $C}		"	\\M\\U\\L\\B2	AR,AL\n":16;
*=[s] HDST,HSRC		{$L $C}		"	\\M\\U\\L\\H2	AR,AL\n":16;
*=[iuipus] WDST,WSRC	{$L $C}		"	mulw2	AR,AL\n":16;
